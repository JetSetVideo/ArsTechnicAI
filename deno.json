{
  "nodeModulesDir": "auto",
  "tasks": {
    "install": "deno install --allow-scripts && deno run -A scripts/patch-nextjs.ts",
    "postinstall": "deno run -A scripts/patch-nextjs.ts",
    "dev": "deno run -A npm:next@14.2.15 dev",
    "build": "deno run -A npm:next@14.2.15 build",
    "start": "deno run -A npm:next@14.2.15 start",
    "lint": "deno run -A npm:next@14.2.15 lint",
    "test": "deno run -A npm:vitest run",
    "test:watch": "deno run -A npm:vitest",
    "test:coverage": "deno run -A npm:vitest run --coverage"
  },
  "allowScripts": ["npm:@prisma/client", "npm:@prisma/engines", "npm:prisma", "npm:unrs-resolver"]
}
